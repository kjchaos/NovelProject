.search-container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx;
}
.search-container .search {
  width: 500rpx;
  border: 1px solid #FFDE00;
  padding: 15rpx 20rpx;
  border-radius: 10rpx;
  color: #808080;
  margin-right: 15rpx;
}
.search-container .search-btn {
  padding: 16rpx 15rpx;
  border-radius: 6rpx;
  background-color: #FFDE00;
  color: #fff;
}

